local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Nico Hub",
   Icon = 0,
   LoadingTitle = "Nico Hub",
   LoadingSubtitle = "by ocin0978",
   ShowText = "Rayfield",
   Theme = "Midnight",

   ToggleUIKeybind = "K",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Nico Hub"
   },

   Discord = {
      Enabled = false,
      Invite = "link",
      RememberJoins = true
   },

   KeySystem = false,
   KeySettings = {
      Title = "Nico Hub",
      Subtitle = "Nico Hub Key System",
      Note = "",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"},
   }
})

local Tab = Window:CreateTab("Script Hub", "file-code-2")

local Section = Tab:CreateSection("Admin Scripts")

local Button = Tab:CreateButton({
   Name = "Infinite Yield",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
   end,
})

local Section = Tab:CreateSection("Scripts")

local Button = Tab:CreateButton({
   Name = "Anti Kick/Anti Ban",
   Callback = function()
   local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

hookfunction(LocalPlayer.Kick, function(...)
    return nil
end)

local mt = getrawmetatable(game)
setreadonly(mt, false)
local old = mt.__namecall

mt.__namecall = newcclosure(function(self, ...)
    local method = getnamecallmethod()
    if method == "Kick" and self == LocalPlayer then
        return nil
    end
    if tostring(self):lower():find("ban") then
        return nil
    end
    return old(self, ...)
end)

for _, v in pairs(ReplicatedStorage:GetDescendants()) do
    if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
        if v.Name:lower():find("ban") then
            v:Destroy()
        end
    end
end
   end,
})

local Tab = Window:CreateTab("Games", "gamepad-2")

local Section = Tab:CreateSection("Dead Rails")

local Button = Tab:CreateButton({
   Name = "Ringta",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/wefwef127382/DEADRAILS.github.io/refs/heads/main/mainringta.lua"))()
   end,
})

local Section = Tab:CreateSection("99 Nights In Forest")

local Button = Tab:CreateButton({
   Name = "Voidware",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/VapeVoidware/VWExtra/main/NightsInTheForest.lua", true))()
   end,
})
