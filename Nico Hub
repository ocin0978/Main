local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Nico Hub",
   Icon = 0,
   LoadingTitle = "Nico Hub",
   LoadingSubtitle = "by ocin0978",
   ShowText = "Rayfield",
   Theme = "Default",

   ToggleUIKeybind = "K",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Nico Hub"
   },

   Discord = {
      Enabled = false,
      Invite = "link",
      RememberJoins = true
   },

   KeySystem = false,
   KeySettings = {
      Title = "Nico Hub",
      Subtitle = "Nico Hub Key System",
      Note = "",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"},
   }
})

